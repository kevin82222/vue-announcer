{
  "name": "@vue-a11y/announcer",
  "version": "3.1.5",
  "types": "dist/index.d.ts",
  "main": "dist/announcer.cjs.js",
  "style": "dist/style.css",
  "browser": "dist/announcer.es.js",
  "module": "dist/announcer.es.js",
  "unpkg": "dist/announcer.iife.js",
  "jsdelivr": "dist/announcer.iife.js",
  "scripts": {
    "dev": "vite",
    "build": "npm-run-all -s clean build:*",
    "build:vite": "vue-tsc --noEmit && vite build",
    "build:types": "tsc --emitDeclarationOnly",
    "demo:dev": "cd demo && vite",
    "demo:build": "cd demo && yarn build:vite",
    "demo:publish": "surge ./demo/dist https://vue-announcer.surge.sh/",
    "clean": "rm -rf dist",
    "release": "standard-version",
    "project:publish": "git push --tags origin next && yarn build && npm publish --access public --tag next"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.21",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vue/compiler-sfc": "^3.0.11",
    "npm-run-all": "^4.1.5",
    "standard-version": "^9.1.0",
    "typescript": "^4.1.3",
    "vite": "^2.1.5",
    "vue-router": "^4.0.6",
    "vue-tsc": "^0.0.21"
  }
}
