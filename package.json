{
  "name": "vue-announcer",
  "version": "1.0.6",
  "description": "A simple way with Vue to announce any information to the screen readers.",
  "main": "dist/vue-announcer.js",
  "scripts": {
    "dev": "rollup --config rollup.config.dev.js --watch",
    "dist": "rollup --config rollup.config.prod.js --environment NODE_ENV:production",
    "release": "standard-version",
    "test": "node_modules/.bin/cypress run",
    "test:open": "node_modules/.bin/cypress open",
    "projectPublish": "git push --follow-tags origin master && npm publish",
    "publishDemo": "surge example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-a11y/vue-announcer.git"
  },
  "keywords": [
    "announcer",
    "Vue.js",
    "Vue",
    "accessibility",
    "a11y",
    "screen",
    "readers",
    "Vision",
    "Disability",
    "JAWS",
    "ChromeVox",
    "NVDA"
  ],
  "author": "Alan Ktquez <ktquez@gmail.com> (https://medium.com/@ktquez)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vue-a11y/vue-announcer/issues"
  },
  "homepage": "https://github.com/vue-a11y/vue-announcer#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "chokidar": "^3.3.1",
    "cypress": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.1.2",
    "eslint-plugin-vue-a11y": "0.0.31",
    "jest": "^25.1.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-vue": "^5.1.1",
    "standard-version": "^7.1.0",
    "surge": "^0.21.3",
    "vue": "^2.6.11",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11"
  }
}
